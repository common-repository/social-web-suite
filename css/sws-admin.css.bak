/* Modal */
.sws-modal
{
	position: fixed;
	overflow: auto;
	height: 100%;
	width: 100%;
	top: 0;
	z-index: 100000;
	display: none;
	background: rgba(0,0,0,0.6)
}
.sws-modal .sws-modal-dialog
{
	background: transparent;
	position: absolute;
	left: 50%;
